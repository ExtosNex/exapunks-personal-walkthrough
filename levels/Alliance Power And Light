;GET HOST NAMES
;SAVE VAR TO X AND T
GRAB 300
COPY F X
COPY F T





LINK 800
REPL SPAWNER
LINK -1
COPY 0 X

MARK WAIT
NOOP
ADDI X 1 X
TEST X = 5
FJMP WAIT

COPY 0 M
COPY 0 M

HALT

;SPAWNER
MARK SPAWNER
;PINCER MOVEMENT
REPL NORTH_T ;T VAR
REPL SOUTH_X ;X VAR
;@REP 3
;NOOP
;@END
;REPL NORTH_X
;REPL SOUTH_T


LINK 801
JUMP SPAWNER


MARK NORTH_T
LINK 802
LINK 802
COPY T X
HOST T
TEST X = T
TJMP WRITE
MARK SEEK_NT
LINK 800
HOST T
TEST X = T
FJMP SEEK_NT
TJMP WRITE



MARK SOUTH_X
LINK 800
LINK 800
HOST T
TEST X = T
TJMP WRITE
MARK SEEK_SX
LINK 802
HOST T
TEST X = T
FJMP SEEK_SX
TJMP WRITE



;MARK NORTH_X
;COPY T X
;HOST T
;TEST X = T
;TJMP WRITE
;MARK SEEK_NX
;LINK 802
;HOST T
;TEST X = T
;FJMP SEEK_NX
;TJMP WRITE



;MARK SOUTH_T
;HOST T
;TEST X = T
;TJMP WRITE
;MARK SEEK_ST
;LINK 800
;HOST T
;TEST X = T
;FJMP SEEK_ST
;TJMP WRITE









MARK WRITE
COPY M #POWR



