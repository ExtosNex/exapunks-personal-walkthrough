;NORTH 800
;EAST 801
;SOUTH 802
;WEST 803
GRAB 300
LINK 800
;FIRST COORD X
;SECOND COORD Y



;CHECK X
SEEK 1
COPY F X 
;CHECK IF NO MVMT
TEST X = 0
TJMP Y_AXIS

;MOVE ALONG X AXIS
MARK X_AXIS
LINK 801
SUBI X 1 X
TEST X = 0
FJMP X_AXIS



;CHECK Y AXIS
MARK Y_AXIS
COPY F X
;CHECK IF NO MVMT
TEST X = 0
TJMP WRITE
;TEST IF NORTH
TEST X > 0
TJMP POS_Y
;IF FAIL THEN SOUTH


;MOVE ALONG Y AXIS
;MOVE SOUTH
MARK NEG_Y
LINK 802
ADDI X 1 X
TEST X = 0
FJMP NEG_Y
TJMP WRITE

;MOVE NORTH
MARK POS_Y
LINK 800
SUBI X 1 X
TEST X = 0
FJMP POS_Y





MARK WRITE
;GRAB RESTAURANT NAME
SEEK -9999
COPY F X
WIPE
GRAB 200
;LOOK FOR REST~ ENTRY
MARK PARSER
TEST F = X 
FJMP PARSER
;WHEN TRUE
;COPY & PASTE ASTERISK
COPY F T
@REP 4
COPY T F 
@END

HALT
