;UNLOCK DISC DRIVE
LINK 800
COPY 8 #AUTH
COPY 0 #AUTH
COPY 3 #AUTH
COPY 2 #AUTH
COPY 7 #AUTH
COPY 1 #AUTH
COPY 0 #AUTH
COPY 4 #AUTH
COPY 9 #AUTH
COPY 5 #AUTH
COPY 1 #AUTH
COPY 2 #AUTH
COPY 5 #AUTH
COPY 2 #AUTH
COPY 6 #AUTH



;BRANCH COPY FILE
REPL SCRIBE


;FIND DISC
MARK DISC
COPY #TRAK X
LINK 801
GRAB X
MARK READ
COPY F M
TEST EOF
FJMP READ
;9999 IS EOF INDICATOR
COPY -9999 M
DROP
LINK -1
REPL SCRIBE
;WAIT HERE BECUASE SOME
;FILES ARE SMOL
@REP 17
NOOP
@END
JUMP DISC







MARK SCRIBE
;GET REGION KEY
LINK -1
GRAB 300
COPY F X
DROP

LINK 800
LINK 800
MAKE
MARK SCRIBE_LOOP
COPY M T
COPY T F
TEST T = -9999
FJMP SCRIBE_LOOP
SEEK -1
VOID F
;REPLACE REGION CODE
SEEK -9999
MARK PARSE
;TEST IF WORD OR NUMBER
TEST F > 0
TJMP PARSE
SEEK -1
COPY X F
TEST EOF
FJMP PARSE
HALT
